{
  "name": "diceus",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "start:app": "ng serve",
    "dev": "concurrently -k \"npm:start:api\" \"npm:start:app\"",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "test:e2e": "start-server-and-test dev http-get://localhost:4200 e2e",
    "start:api": "json-server --watch seed.json --port 3001 --delay 400"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.1",
    "@angular/cdk": "^20.2.4",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.4",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "chart.js": "^4.5.0",
    "ng2-charts": "^8.0.0",
    "ngx-toastr": "^19.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.0",
    "@testing-library/cypress": "^10.1.0",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.3.0",
    "concurrently": "^9.2.1",
    "cypress": "^15.3.0",
    "eslint": "^9.35.0",
    "jasmine-core": "~5.9.0",
    "json-server": "^1.0.0-beta.3",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.40.0"
  }
}
